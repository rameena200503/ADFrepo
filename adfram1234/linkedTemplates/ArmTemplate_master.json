{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfram1234"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"ls_Azure_SqlServer_TARGET_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_Azure_SqlServer_TARGET'"
		},
		"ls_adls_ram_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_ram'"
		},
		"ls_azure_sqlServer_SOURCE_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure_sqlServer_SOURCE'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ramdevazureacc.dfs.core.windows.net/"
		},
		"ls_Azure_SqlServer_TARGET_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ramsql123.database.windows.net"
		},
		"ls_Azure_SqlServer_TARGET_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "TARGET"
		},
		"ls_Azure_SqlServer_TARGET_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ramserver12"
		},
		"ls_adls_ram_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsram123.dfs.core.windows.net/"
		},
		"ls_azure_sqlServer_SOURCE_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ramsql123.database.windows.net"
		},
		"ls_azure_sqlServer_SOURCE_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SOURCE"
		},
		"ls_azure_sqlServer_SOURCE_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "ramserver12"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfram1234_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"ls_Azure_SqlServer_TARGET_password": {
						"value": "[parameters('ls_Azure_SqlServer_TARGET_password')]"
					},
					"ls_adls_ram_accountKey": {
						"value": "[parameters('ls_adls_ram_accountKey')]"
					},
					"ls_azure_sqlServer_SOURCE_password": {
						"value": "[parameters('ls_azure_sqlServer_SOURCE_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"ls_Azure_SqlServer_TARGET_properties_typeProperties_server": {
						"value": "[parameters('ls_Azure_SqlServer_TARGET_properties_typeProperties_server')]"
					},
					"ls_Azure_SqlServer_TARGET_properties_typeProperties_database": {
						"value": "[parameters('ls_Azure_SqlServer_TARGET_properties_typeProperties_database')]"
					},
					"ls_Azure_SqlServer_TARGET_properties_typeProperties_userName": {
						"value": "[parameters('ls_Azure_SqlServer_TARGET_properties_typeProperties_userName')]"
					},
					"ls_adls_ram_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_ram_properties_typeProperties_url')]"
					},
					"ls_azure_sqlServer_SOURCE_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_sqlServer_SOURCE_properties_typeProperties_server')]"
					},
					"ls_azure_sqlServer_SOURCE_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_sqlServer_SOURCE_properties_typeProperties_database')]"
					},
					"ls_azure_sqlServer_SOURCE_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_sqlServer_SOURCE_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfram1234_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfram1234_ArmTemplate_0')]"
			]
		}
	]
}